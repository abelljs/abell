{
  "name": "abell",
  "version": "1.0.0-alpha.56",
  "description": "",
  "bin": "./dist/cjs/bin.js",
  "main": "./dist/cjs/index.js",
  "browser": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist",
    "defaults",
    "types.d.ts"
  ],
  "scripts": {
    "test": "vitest",
    "test:once": "vitest run",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --project tsconfig.build.json --outDir dist/cjs",
    "build:esm": "tsc --project tsconfig.build.json --allowSyntheticDefaultImports --module esnext --target esnext --outDir dist/esm",
    "postbuild": "node ../../scripts/post-build.js abell",
    "eslint": "eslint",
    "dev": "nodemon --exec \"yarn build\" --watch src -e js,ts",
    "prepublishOnly": "yarn test:once && yarn eslint && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abelljs/abell.git"
  },
  "keywords": [
    "blog",
    "ssg",
    "gatsby",
    "jekyll",
    "static",
    "website",
    "generator"
  ],
  "author": "saurabhdaware",
  "bugs": {
    "url": "https://github.com/abelljs/abell/issues"
  },
  "homepage": "https://abelljs.org",
  "license": "MIT",
  "dependencies": {
    "commander": "^7.2.0",
    "express": "^4.17.2",
    "postcss-selector-parser": "^6.0.10",
    "stylis": "^4.1.0",
    "vite": "2.9.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/stylis": "^4.0.2",
    "nodemon": "^2.0.16"
  }
}
